@model string[,]
@{
    ViewBag.Title = "ShowUploadImage";
}

<ul>
    @for (int i=0;i<Model.Length;i++)
    {
        <li>
            <a href="#"><img src="@Model[i,0]" alt="" /></a>
            <div class="image-hover">
                <h6>@Model[i, 1]</h6>
                <a href="#" class="preview">Preview image</a>
            </div>
        </li>
    }
</ul>
